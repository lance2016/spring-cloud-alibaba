<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.server.order.mapper.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cloud.server.order.entity.Order">
        <result column="id" property="id" />
        <result column="buyer" property="buyer" />
        <result column="product_id" property="productId" />
        <result column="product_name" property="productName" />
        <result column="create_time" property="createTime" />
        <result column="status" property="status" />
        <result column="is_delete" property="isDelete" />
        <result column="update_time" property="updateTime" />
        <result column="num" property="num"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, buyer, product_id, product_name, create_time, status, is_delete, update_time, num
    </sql>

</mapper>
